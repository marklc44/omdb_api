<h1>Favorites</h1>

<ul class="results">
	<% faves.forEach(function(movie, index) { %>
		<li>
			<h4><a href="/movies/<%= movie.id %>"><%= movie.title %></a></h4>
			<form method="post" action="/favorites/<%= movie.id %>?_method=delete">
				<input class="unfavorite" type="submit" value="">
			</form>
		</li>
	<% }) %>
</ul>